%define INSTANCEHOME ${client1:location}
instancehome $INSTANCEHOME
%define CLIENTHOME ${buildout:directory}\var\client1
clienthome $CLIENTHOME

products ${client1:products}
debug-mode ${client1:debug-mode}
security-policy-implementation C
verbose-security ${client1:verbose-security}
default-zpublisher-encoding utf-8

effective-user ${client1:effective-user}

zserver-threads 2

<environment>
${client1:environment-vars}    
</environment>

<warnfilter>
  action default
  category exceptions.DeprecationWarning
</warnfilter>


<eventlog>
  
  level INFO
  <logfile>
    path ${client1:event-log}
    level INFO
  </logfile>
</eventlog>

<logger access>
  level WARN
  <logfile>
    path ${client1:z2-log}
    format %(message)s
  </logfile>
</logger>





<zodb_db main>
    # Main database
    cache-size 10000
    
# Blob-enabled ZEOStorage database
    <zeoclient>
      blob-dir ${client1:blob-storage}
      shared-blob-dir ${client1:shared-blob}
      server ${client1:zeo-address}
      storage 1
      name zeostorage
      var ${client1:location}\var
      cache-size 128MB
      
      
      
      
      
      
    </zeoclient>
    mount-point /
</zodb_db>

<zodb_db temporary>
    # Temporary storage database (for sessions)
    <temporarystorage>
      name temporary storage for sessioning
    </temporarystorage>
    mount-point /temp_folder
    container-class Products.TemporaryFolder.TemporaryContainer
</zodb_db>

pid-filename ${client1:pid-file}
lock-filename ${client1:lock-file}
python-check-interval 1000
enable-product-installation off


