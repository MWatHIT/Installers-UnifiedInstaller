<?xml version="1.0" encoding="utf-8" ?>
<MSDeploy.iisApp>
  <iisApp path="PloneInstaller-4-2" />
  <runCommand
      path="&quot;%SystemDrive%\Python27\python.exe&quot; &quot;%APPL_PHYSICAL_PATH%\zinstance\bootstrap.py&quot; -d -c &quot;%APPL_PHYSICAL_PATH%\zinstance\buildout.cfg&quot;"
      MSDeploy.MSDeployKeyAttributeName="path" />
  <runCommand
      path="&quot;%APPL_PHYSICAL_PATH%\zinstance\bin\buildout.exe&quot; -c &quot;%APPL_PHYSICAL_PATH%\zinstance\buildout.cfg&quot; -N"
      MSDeploy.MSDeployKeyAttributeName="path" />
  <runCommand
      path="&quot;%APPL_PHYSICAL_PATH%\zinstance\bin\instance.exe&quot; adduser admin &quot;%ADMIN_PASSWORD%&quot;"
      MSDeploy.MSDeployKeyAttributeName="path" />
  <runCommand
      path="&quot;%IIS_BIN%\appcmd.exe&quot; set config -section:system.webServer/fastCgi /+&quot;[fullPath='%SystemDrive%\Python27\python.exe',arguments='-u %APPL_PHYSICAL_PATH%\zinstance\bin\iisfcgi-script.py -c %APPL_PHYSICAL_PATH%\zinstance
