      <installer>
        <id>1</id>
        <!-- List of languages this package is localized in -->
        <languageId>en</languageId>
        <osList idref="SupportedAppPlatforms" />
        <installerFile>
          <!-- size of the package in KBs -->
          <fileSize>50759</fileSize>
          <installerURL>https://github.com/downloads/plone/Installers-UnifiedInstaller/Plone42.zip</installerURL>
          <displayURL>http://plone.org/products/plone//releases/4.2</displayURL>
